<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="600.0" spacing="10" style="-fx-background-color: #89CFF0; -fx-background-radius: 10px;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sudokusolver.ui.RecentSolveController">
    <Label style="-fx-text-fill: #4169E1;" styleClass="label-title" text="SudokuSolver">
      <font>
         <Font name="System Bold Italic" size="36.0" />
      </font></Label>
    <Button onAction="#handleRecentSolve1" prefWidth="220.0" style="-fx-background-radius: 15px; -fx-background-color: #FF7F50;" text="RecentSolve1">
      <font>
         <Font name="System Bold" size="16.0" />
      </font>
      <VBox.margin>
         <Insets top="50.0" />
      </VBox.margin></Button>
    <Button onAction="#handleRecentSolve2" prefWidth="220.0" style="-fx-background-radius: 15px; -fx-background-color: #FF7F50;" text="RecentSolve2">
      <font>
         <Font name="System Bold" size="16.0" />
      </font></Button>
    <Button onAction="#handleRecentSolve3" prefWidth="220.0" style="-fx-background-radius: 15px; -fx-background-color: #FF7F50;" text="RecentSolve3">
      <font>
         <Font name="System Bold" size="16.0" />
      </font></Button>
    <Button onAction="#handleRecentSolve4" prefWidth="220.0" style="-fx-background-radius: 15px; -fx-background-color: #FF7F50;" text="RecentSolve4">
      <font>
         <Font name="System Bold" size="16.0" />
      </font></Button>
    <Button onAction="#handleRecentSolve5" prefWidth="220.0" style="-fx-background-radius: 15px; -fx-background-color: #FF7F50;" text="RecentSolve5">
      <font>
         <Font name="System Bold" size="16.0" />
      </font></Button>
    <Button onAction="#handleHome" prefWidth="220.0" style="-fx-background-radius: 15px; -fx-background-color: #FF7F50;" text="Home">
      <font>
         <Font name="System Bold" size="16.0" />
      </font></Button>
</VBox>
